{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
        "no-console": 0
    }
}